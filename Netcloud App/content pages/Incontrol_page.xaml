<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Netcloud_App.content_pages.Incontrol_page">
  <!--Pages can be added as references or inline-->
    <ContentPage Title="Offline events" >
        <ScrollView>
            <StackLayout x:Name="offline_stack">
            </StackLayout>
        </ScrollView>
    </ContentPage>
    <ContentPage Title="Bandwidth events">
        <ScrollView>
            <StackLayout x:Name="bandwidth_stack">
            </StackLayout>
        </ScrollView>
    </ContentPage>
</TabbedPage>